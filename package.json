{
  "name": "@moxy/next-intl",
  "version": "0.0.0",
  "description": "Library to integrate react-intl with Next.js, making it easy to manage the current locale based on configurable policies",
  "main": "es/index.js",
  "module": "es/index.js",
  "browser": {
    "es/react/script.js": "./es/react/script.browser.js"
  },
  "files": [
    "es"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "test": "jest",
    "build": "BABEL_ENV=es babel src -d es --delete-dir-on-start",
    "prerelease": "npm t && npm run lint && npm run build",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "locale",
    "localization",
    "intl",
    "l10n",
    "i18n",
    "isomorphic",
    "language"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/react-intl.git"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.3.2",
    "@formatjs/intl-relativetimeformat": "^4.4.1",
    "accept-language-parser": "^1.5.0",
    "p-cancelable": "^2.0.0",
    "pico-signals": "^1.0.0",
    "universal-cookie": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-intl": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@moxy/jest-config": "^1.1.0",
    "@testing-library/react": "^9.3.2",
    "babel-jest": "^24.9.0",
    "babel-preset-moxy": "^3.2.0",
    "delay": "^4.3.0",
    "eslint": "^6.6.0",
    "eslint-config-moxy": "^9.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^3.6.0",
    "standard-version": "^7.0.0"
  }
}
